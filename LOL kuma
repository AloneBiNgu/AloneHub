local getService = setmetatable({}, {
	__newindex = function()
		while true do
		end
	end,
	__tostring = function()
		while true do
		end
	end,
	__call = function(self, serviceName)
		return (function(...)
			return ...
		end)(game.GetService(game, serviceName))
	end
})

